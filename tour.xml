	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
	</action>
	<!--Remove up and down controls -->
	<control  keycodesup="" keycodesdown=""/>
	
	<events onkeydown="if(keycode == 38, loadFacingPano(););"	/>
	
	<action name="loadFacingPano">
		if(get(view.hlookat) GT -22.5 AND get(view.hlookat) LE 22.5, loadNorthPano());
		if(get(view.hlookat) GT 22.5 AND get(view.hlookat) LE 67.5, loadNorthEastPano());
		if(get(view.hlookat) GT 67.5 AND get(view.hlookat) LE 112.5, loadEastPano());
		if(get(view.hlookat) GT 112.5 AND get(view.hlookat) LE 157.5, loadSouthEastPano());
		if(get(view.hlookat) GT 157.5 OR get(view.hlookat) LE -157.5, loadSouthPano());
		if(get(view.hlookat) GT -157.5 AND get(view.hlookat) LE -112.5, loadSouthWestPano());
		if(get(view.hlookat) GT -112.5 AND get(view.hlookat) LE -67.5, loadWestPano());
		if(get(view.hlookat) GT -67.5 AND get(view.hlookat) LE -22.5, loadNorthWestPano());
	</action>


	

	
